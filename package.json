{
  "name": "more_recipes",
  "version": "1.0.0",
  "description": "Andela bootcamp project",
  "engines": {
    "node": "8.9.0"
  },
  "main": "server.js",
  "nyc": {
    "exclude": [
      "server/tests/seeder",
      "server/conig/config.js"
    ]
  },
  "scripts": {
    "test": "NODE_ENV=test nyc --reporter=html mocha server/tests/*.test.js --compilers js:babel-register && nyc report",
    "test:file": "NODE_ENV=test nyc --reporter=html mocha server/tests/*.test.js --compilers js:babel-register && nyc report",
    "test2": "mocha server/tests/*.test.js --compilers js:babel-register",
    "start": "NODE_ENV=production node index",
    "start:dev": "nodemon server --exec babel-node",
    "migration:test": "NODE_ENV=test sequelize db:migrate",
    "undo:migration:test": "NODE_ENV=test sequelize db:migrate:undo:all",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codestaintin/More_Recipes.git"
  },
  "author": "Isioye Mohammed",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "coveralls": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^7.4.3",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.2.1",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.8.0",
    "sequelize-cli": "^2.8.0",
    "validatorjs": "^3.13.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.0",
    "should": "^13.0.1",
    "supertest": "^3.0.0"
  }
}
