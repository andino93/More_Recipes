{
  "name": "more_recipes",
  "version": "1.0.0",
  "description": "Andela bootcamp project",
  "engines": {
    "node": "8.4.0"
  },
  "main": "server.js",
  "nyc": {
    "exclude": [
      "server/tests/seeder"
    ]
  },
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack-dev-server",
    "test": "npm run test:client && npm run test:server",
    "test:server": "export NODE_ENV=test && sequelize db:migrate:undo:all && sequelize db:migrate && nyc --reporter=html mocha server/tests/*.spec.js --compilers js:babel-register && nyc report",
    "start": "node tools/buildTools/publicServer.js",
    "heroku-prebuild": "npm install",
    "heroku-postbuild": "NODE_ENV=production webpack -p --config ./webpack.config.prod.js --progress",
    "start:dev": "nodemon --exec babel-node server.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:client": "jest /client --coverage --collectCoverageFrom=client/**/**/*.{js,jsx}",
    "e2e-server": "selenium-standalone start",
    "e2e-setup": "./node_modules/selenium-standalone/bin/selenium-standalone install",
    "migrate": "NODE_ENV=test && sequelize db:migrate:undo:all && sequelize db:migrate",
    "test:e2e": "npm run migrate && nightwatch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codestaintin/More_Recipes.git"
  },
  "author": "Isioye Mohammed",
  "license": "ISC",
